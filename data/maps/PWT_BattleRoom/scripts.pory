mapscripts PWT_BattleRoom_MapScripts {
    MAP_SCRIPT_ON_TRANSITION : PWT_BattleRoom_OnTransition
    MAP_SCRIPT_ON_FRAME_TABLE [VAR_TEMP_0, 0: PWT_BattleRoom_EventScript_EnterRoom]
    MAP_SCRIPT_ON_WARP_INTO_MAP_TABLE [VAR_TEMP_1, 0: PWT_BattleRoom_EventScript_SetUpObjects]
    MAP_SCRIPT_ON_RESUME : PWT_BattleRoom_OnResume
}

script PWT_BattleRoom_OnTransition {
	setflag(FLAG_TEMP_HIDE_FOLLOWER)
	pwt_SetOpponentGFX
	pwt_GetData(PWT_DATA_ROUND_NUM)
	copyvar(VAR_TEMP_F, VAR_RESULT)
	call_if_eq(VAR_RESULT, PWT_ROUND1, PWT_BattleRoom_EventScript_SetWalkingAudienceMemberPos)
	setoutfitgfxintovar(VAR_OBJ_GFX_ID_1)
	end
}

script PWT_BattleRoom_EventScript_EnterRoom {
	lockall
	pwt_GetData(PWT_DATA_ROUND_NUM)
	call_if_eq(VAR_RESULT, PWT_ROUND1, PWT_BattleRoom_EventScript_TryDoAudienceMemberWalkToSeat)
	applymovement(LOCALID_PWT_ANNOUNCER, Common_Movement_WalkInPlaceDown)
	waitmovement(0)
	// Player comes into the arena
	call(PWT_BattleRoom_EventScript_AnnouncePlayer)
	setspeaker("Announcer")
	message("The Littleroot Hero,\n{PLAYER} has entered!$")
	waitbuttonpress
	closemessage
	showobjectat(LOCALID_PWT_PLAYER, MAP_PWT_BATTLE_ROOM)
	applymovement(LOCALID_PWT_PLAYER, moves(set_visible, delay_16, walk_up * 3, walk_right * 2))
	// Audience reacts
	playse(SE_M_ENCORE2)
	call(PWT_BattleRoom_EventScript_AudienceLookAround)
	waitmovement(0)
	waitse
	// Opponent comes into the arena
	setspeaker("Announcer")
	pwt_AnnounceTheOpponent
	waitbuttonpress
	closemessage
	showobjectat(LOCALID_PWT_OPPONENT, MAP_PWT_BATTLE_ROOM)
	applymovement(LOCALID_PWT_OPPONENT, moves(set_visible, delay_16, walk_up * 3, walk_left * 2))
	// Audience reacts
	playse(SE_M_ENCORE2)
	call(PWT_BattleRoom_EventScript_AudienceLookAround)
	waitmovement(0)
	waitse
	// Prepare to battle
	pwt_BufferBattleStrings
	setspeaker("Announcer")
	msgbox("{STR_VAR_1}\n{PLAYER} versus {STR_VAR_2}!\lLet the battle begin!$", MSGBOX_DEFAULT)
	closemessage
	applymovement(LOCALID_PWT_PLAYER, moves(walk_right))
	applymovement(LOCALID_PWT_OPPONENT, moves(walk_left))
	waitmovement(0)
	// Meet opponent
	setspeaker("{STR_VAR_2}")
	pwt_ShowOpponentIntro
	waitmessage
	closemessage
	// Start battle
	pwt_SetOpponentParty
	call(PWT_BattleRoom_EventScript_DoPWTBattle)
	releaseall
	end
}
